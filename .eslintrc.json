{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-shadow": 0,
        "prefer-arrow-callback": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0
    }
}
